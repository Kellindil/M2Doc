<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.obeonetwork.m2doc.rcptt.ui.assertEqualsDocx"
            name="Assert docx are equals">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.obeonetwork.m2doc.rcptt.ui.command.AssertEqualsHandler"
            commandId="org.obeonetwork.m2doc.rcptt.ui.assertEqualsDocx">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="org.obeonetwork.m2doc.rcptt.ui.assertEqualsDocx"
               label="Assert docx are equals"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
