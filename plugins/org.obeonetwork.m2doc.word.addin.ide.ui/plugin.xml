<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.obeonetwork.m2doc.word.addin.ide.ui.commands.addInHandlerCommand"
            name="Start/Stop M2Doc add-in back end">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.obeonetwork.m2doc.word.addin.ide.ui.handlers.AddInHandler"
            commandId="org.obeonetwork.m2doc.word.addin.ide.ui.commands.addInHandlerCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.obeonetwork.m2doc.word.addin.ide.ui.toolbars.sampleToolbar">
            <command
                  commandId="org.obeonetwork.m2doc.word.addin.ide.ui.commands.addInHandlerCommand"
                  icon="icons/m2doc.png"
                  id="org.obeonetwork.m2doc.word.addin.ide.ui.toolbars.addInHandlerCommand"
                  label="Start/Stop M2Doc add-in back end"
                  style="toggle"
                  tooltip="Start/Stop M2Doc add-in back end">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
